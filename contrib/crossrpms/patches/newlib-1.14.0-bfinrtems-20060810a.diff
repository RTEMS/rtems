+int
+_swiopen (const char * path,
+	  int          flags)
+{
+  int aflags = 0, fh;
+  int block[3];
+  
+  int i = findslot (-1);
+  
+  if (i == MAX_OPEN_FILES)
+    return -1;
+
+  block[0] = (int) path;
+  block[1] = flags;
+
+  fh = do_syscall (SYS_open, block);
+
+  if (fh >= 0)
+    {
+      openfiles[i].handle = fh;
+      openfiles[i].pos = 0;
+    }
+
+  return fh >= 0 ? fh + FILE_HANDLE_OFFSET : error (fh);
+}
